# ~/.bash/optprogs
source ~/.bash/functions

# blender
pathmunge /opt/blender/2.79 after

# paraview
#pathmunge /opt/paraview/5.5.0/bin after

# tecplot
pathmunge /opt/tecplot/bin after
TECPHYFILE=~/.Trash/tecplot.phy
export TECPHYFILE

# android sdk (adb, fastboot)
pathmunge /opt/android-sdk/platform-tools/

# zoom
pathmunge /opt/zoom/ after
